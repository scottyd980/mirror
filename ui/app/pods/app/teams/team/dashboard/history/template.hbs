<div class="container">
  <div class="row">
    <div class="col-md-12">
      {{#x-card title="History"}}
        {{#if model.retrospectives}}
          {{team-history-list
            retrospectives=model.retrospectives
            isAdmin=isAdmin
            onConfirm=(route-action "confirmAction")
            onDelete=(route-action "deleteAction")
          }}
        {{else}}
          It doesn't look like this team has had any retrospectives yet.
        {{/if}}
      {{/x-card}}
    </div>
  </div>
</div>
