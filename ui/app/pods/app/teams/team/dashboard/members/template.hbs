<div class="container">
  <div class="row">
    <div class="col-md-12">
      {{#x-card title="Members"}}
        {{team-member-list
          members=model.members
          team=model.team
          currentUser=model.currentUser
          isAdmin=isAdmin
          onConfirm=(route-action "confirmAction")
          onMakeAdmin=(route-action "addAdmin")
          onRemoveAdmin=(route-action "removeAdmin")
          onDelete=(route-action "deleteMember")}}
      {{/x-card}}
    </div>
  </div>
</div>
